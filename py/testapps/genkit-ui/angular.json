{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "analytics": false
    },
    "newProjectRoot": "projects",
    "projects": {
        "genkit-ui": {
            "projectType": "library",
            "root": "",
            "sourceRoot": "src",
            "architect": {
                "storybook": {
                    "builder": "@storybook/angular:start-storybook",
                    "options": {
                        "configDir": ".storybook",
                        "browserTarget": "genkit-ui:build-storybook",
                        "compodoc": false,
                        "port": 6006
                    }
                },
                "build-storybook": {
                    "builder": "@storybook/angular:build-storybook",
                    "options": {
                        "configDir": ".storybook",
                        "outputDir": "storybook-static",
                        "compodoc": false
                    }
                }
            }
        }
    }
}