{
  "name": "@google-genkit/dotprompt",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "check": "tsc",
    "compile": "tsup",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "watch": "tsup --watch",
    "lint": "eslint --config ../.eslintrc.js --ext .ts src",
    "lint:fix": "npm run lint -- --fix --quiet",
    "test": "node --loader tsx --test tests/*_test.ts"
  },
  "dependencies": {
    "@google-genkit/ai": "*",
    "@types/handlebars": "^4.1.0",
    "front-matter": "^4.0.2",
    "handlebars": "^4.7.8",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.cjs",
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    }
  }
}
