{
  "name": "genkit",
  "version": "0.5.0-rc.6",
  "description": "CLI for interacting with the Google GenKit AI framework",
  "keywords": [
    "genkit",
    "ai",
    "genai",
    "generative-ai"
  ],
  "bin": {
    "genkit": "dist/bin/genkit.js"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm install && pnpm build:genversion && pnpm build:common && pnpm build:cli && pnpm build:ui && pnpm copy-ui-build && pnpm build:plugins",
    "build:cli:watch": "tsc --watch",
    "build:cli": "tsc",
    "build:common": "cd common && pnpm build",
    "build:genversion": "genversion -es src/utils/version.ts",
    "build:plugins": "pnpm -r --workspace-concurrency 8 -F \"./plugins/**\" build",
    "build:ui": "cd ui && pnpm build",
    "copy-ui-build": "rm -rf dist/client && cd ui && rsync -r dist ../dist/client",
    "test:all": "pnpm run test:ui && pnpm run test:jest",
    "test:ui": "cd ui && pnpm test-headless",
    "test:jest": "jest --verbose",
    "export:schemas": "ts-node scripts/schema-exporter.ts .",
    "pack:all": "pnpm run pack:cli && pnpm run pack:common && pnpm run pack:plugins",
    "pack:common": "cd common && pnpm pack --pack-destination ../../dist",
    "pack:cli": "pnpm pack --pack-destination ../dist",
    "pack:plugins": "for i in plugins/*/; do cd $i && pnpm pack --pack-destination ../../../dist && cd ../..; done"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/genkit.git",
    "directory": "genkit-tools"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "dependencies": {
    "@genkit-ai/tools-common": "workspace:*",
    "colorette": "^2.0.20",
    "commander": "^11.1.0",
    "extract-zip": "^2.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/body-parser": "^1.19.5",
    "@types/cli-color": "^2.0.6",
    "@types/configstore": "^6.0.2",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^8.1.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.11.19",
    "@types/uuid": "^9.0.8",
    "genversion": "^3.2.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.7+sha256.50783dd0fa303852de2dd1557cd4b9f07cb5b018154a6e76d0f40635d6cee019"
}
