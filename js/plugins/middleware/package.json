{
  "name": "@genkit-ai/middleware",
  "description": "Middleware for Genkit",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "google cloud",
    "google ai",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "1.28.0",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rimraf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "tsx --test tests/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/genkit.git",
    "directory": "js/plugins/middleware"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@genkit-ai/google-genai": "workspace:^",
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "dependencies": {
    "mime": "^4.1.0"
  }
}
