    "chromadb": "1.8.1"
  },
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
