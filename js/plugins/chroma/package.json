{
  "name": "genkitx-chromadb",
  "description": "Genkit AI framework plugin for Chroma vector database.",
  "keywords": [
    "genkit",
    "genkit-retriever",
    "genkit-plugin",
    "genkit-indexer",
    "chromadb",
    "vector",
    "embedding",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "0.5.0-rc.5",
  "type": "commonjs",
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/google/genkit.git",
    "directory": "js/plugins/chroma"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "dependencies": {
    "@genkit-ai/ai": "workspace:*",
    "@genkit-ai/core": "workspace:*",
    "chromadb": "^1.7.3",
    "ts-md5": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "default": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts"
    }
  }
}
