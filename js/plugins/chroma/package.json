{
  "name": "genkitx-chromadb",
  "description": "Genkit AI framework plugin for Chroma vector database.",
  "keywords": [
    "genkit",
    "genkit-retriever",
    "genkit-plugin",
    "genkit-indexer",
    "chromadb",
    "vector",
    "embedding",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "1.8.0",
