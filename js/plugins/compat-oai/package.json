{
  "name": "@genkit-ai/compat-oai",
  "description": "Genkit AI framework plugin for OpenAI APIs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-embedder",
    "genkit-model",
    "openai",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "1.21.0",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/genkit.git",
    "directory": "js/plugins/compat-oai"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "dependencies": {
    "openai": "^4.95.0"
  },
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.12.12",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./openai": {
      "require": "./lib/openai/index.js",
      "import": "./lib/openai/index.mjs",
      "types": "./lib/openai/index.d.ts",
      "default": "./lib/openai/index.js"
    },
    "./deepseek": {
      "require": "./lib/deepseek/index.js",
      "import": "./lib/deepseek/index.mjs",
      "types": "./lib/deepseek/index.d.ts",
      "default": "./lib/deepseek/index.js"
    },
    "./xai": {
      "require": "./lib/xai/index.js",
      "import": "./lib/xai/index.mjs",
      "types": "./lib/xai/index.d.ts",
      "default": "./lib/xai/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "openai": [
        "lib/openai"
      ],
      "deepseek": [
        "lib/deepseek"
      ],
      "xai": [
        "lib/xai"
      ]
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rm -rf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "jest --coverage"
  }
}
