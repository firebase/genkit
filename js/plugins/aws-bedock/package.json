{
  "name": "@genkit-ai/aws-bedrock",
  "description": "Genkit AI framework plugin for AWS Bedrock APIs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-model",
    "aws",
    "aws-bedrock",
    "ai",
    "genai",
    "generative-ai",
    "gen-ai"
  ],
  "version": "1.0.0",
  "type": "commonjs",
  "main": "lib/cjs/index.js",
  "module": "./lib/esm/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firebase/genkit.git",
    "directory": "js/plugins/aws-bedrock"
  },
  "author": "genkit",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.941.0"
  },
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "globals": "^16.0.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "npm-run-all": "^4.1.5",
    "tsup": "^8.3.5"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rimraf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch"
  }
}
