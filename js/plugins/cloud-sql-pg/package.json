    "uuid": "^11.1.0"
  },
  "peerDependencies": {
    "genkit": "workspace:^"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/dotenv": "^8.2.3",
    "@types/node": "^20.11.16",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^4.9.0"
  },
  "types": "./lib/index.d.ts",
