{
  "name": "@genkit-ai/checks",
  "description": "Google Checks AI Safety plugins for classifying the safety of text against Checks AI safety policies.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "google cloud",
    "google ai",
    "ai",
    "genai",
    "generative-ai",
    "checks",
    "google checks",
    "guardrails"
  ],
  "version": "1.8.0",
  "type": "commonjs",
  "scripts": {
